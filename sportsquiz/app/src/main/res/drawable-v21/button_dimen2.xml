<selector xmlns:android="http://schemas.android.com/apk/res/android">

<item  android:state_pressed="true">
    <shape android:shape="rectangle">
        <solid android:color="#1f6481"
            />
        <corners android:radius="13dp" />
        <stroke
            android:width="2dp"
            android:color="#144e66"
            />
    </shape>
</item>

<item>
    <shape android:shape="rectangle">
        <solid     android:color="#238cb9"
            />
        <corners android:radius="13dp" />
        <stroke
            android:width="2dp"
            android:color="#176687"
            />
    </shape>
</item>

</selector>